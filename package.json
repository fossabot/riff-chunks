{
  "name": "riff-chunks",
  "version": "3.0.0",
  "description": "Get the chunks of RIFF and RIFX files.",
  "homepage": "https://github.com/rochars/riff-chunks",
  "author": "Rafael da Silva Rocha <rocha.rafaelsilva@gmail.com>",
  "license": "MIT",
  "keywords": [
    "RIFF",
    "RIFX",
    "LIST",
    "server",
    "browser",
    "chunk",
    "parser"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rochars/riff-chunks.git"
  },
  "scripts": {
    "lint": "jshint index.js && jshint test",
    "test": "nyc ./node_modules/mocha/bin/_mocha test --recursive",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "bundle": "webpack",
    "compile": "google-closure-compiler-js dist/riff-chunks.js > dist/riff-chunks-min.js --compilationLevel=ADVANCED",
    "doc": "./node_modules/.bin/jsdoc index.js -d docs",
    "qa": "npm run lint && npm test",
    "pack": "npm run bundle && npm run compile && npm run doc",
    "build": "npm run qa && npm run pack"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "google-closure-compiler-js": "^20170910.0.1",
    "jsdoc": "~3.5.5",
    "jshint": "^2.9.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^3.2.2",
    "string-replace-loader": "^1.3.0",
    "webpack": "^3.8.1"
  },
  "main": "index.js",
  "dependencies": {
    "byte-data": "0.5.1"
  }
}